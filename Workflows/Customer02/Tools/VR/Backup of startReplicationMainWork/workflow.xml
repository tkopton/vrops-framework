    <script encoded="false"><![CDATA[
sourceVC=sourceDatacenter.split("-")[1];
if (sourceVC=="2"){
	rzSource="rz2";
} else {
	rzSource="rz1";
}
site=replicationConfig.getAttributeWithKey(rzSource+"_site").value;
remoteSite=replicationConfig.getAttributeWithKey(rzSource+"_remoteSite").value;
remoteDatastore=replicationConfig.getAttributeWithKey(rzSource+"_remoteDS").value;
temp=remoteSite.uri;
remoteSiteString=temp.toString().split("/");
replicationDestination=(remoteSiteString[2].split(":"))[0];]]></script>
