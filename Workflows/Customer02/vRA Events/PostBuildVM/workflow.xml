    <display-name><![CDATA[get values]]></display-name>
    <script encoded="false"><![CDATA[sla = payload.get("machine").get("properties").get("akh.SLA");
if ((sla==null)||(sla=="")){
	AKHCustom=false;
} else {
	AKHCustom=true;
	var vmName = payload.get("machine").get("name");
	vcvm = Server.findAllForType("VC:VirtualMachine", "xpath:name='" + vmName + "'")[0];
	backupProp=payload.get("machine").get("properties").get("akh.Backup");
	temp=payload.get("machine").get("properties").get("akh.Replication");
	sourceDatacenter=payload.get("machine").get("properties").get("akh.rechenzentrum");
	deploymentId=payload.machine.properties.get("__Cafe.Root.Request.Id");
}
]]></script>
