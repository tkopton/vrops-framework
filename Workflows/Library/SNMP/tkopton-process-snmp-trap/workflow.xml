  <input>
    <param name="trapData" type="Array/Properties"/>
  </input>
  <workflow-item name="item2" out-name="item3" type="task">
    <display-name><![CDATA[Trap received]]></display-name>
    <script encoded="false"><![CDATA[// We extract the host name out of the SNMP TrapData
var HostName;
for (var x = 0; x < trapData.length; x++) {
var prop = trapData[x];
if (prop.get("oid") == HostOID) {
HostName = prop.get("value");
break;
}
}

System.log("Host name: "+ HostName);]]></script>
